{
  "name": "@routeros-rag/network-portal",
  "version": "0.1.0",
  "description": "Modern network monitoring and configuration dashboard for RouterOS devices",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:server\"",
    "dev:web": "next dev --port 3003 --hostname 10.0.0.185",
    "dev:server": "tsx watch server/index.ts",
    "build": "npm run build:web && npm run build:server",
    "build:web": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "npm run start:server",
    "start:web": "next start",
    "start:server": "node dist/server/index.js",
    "db:init": "tsx server/database/init.ts",
    "db:reset": "rm -f network-portal.db && npm run db:init",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist node_modules",
    "db:clear-metrics": "tsx server/scripts/clear-metrics.ts",
    "db:check-traffic": "tsx server/scripts/check-traffic-data.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/html2canvas": "^1.0.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "ansi_up": "^6.0.6",
    "basic-ftp": "^5.0.5",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.10.0",
    "chart.js": "^4.5.0",
    "date-fns": "3.0.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "0.400.0",
    "next": "15.3.3",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.48.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^2.15.3",
    "ssh2": "^1.16.0",
    "uuid": "^11.1.0",
    "ws": "8.16.0",
    "zod": "3.22.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ssh2": "^1.15.5",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@types/xterm": "^3.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "routeros",
    "mikrotik",
    "network",
    "monitoring",
    "dashboard",
    "configuration"
  ]
}
